<div class="row">
  <div class="gift-price col-md-9" ng-show="!gift.is_priceless()">
    <strong>Pre√ßo<span ng-show="gift.has_many()"> Total</span>: </strong>
    <span ng-repeat="price in gift.price_range track by $index">
      <span ng-show="$index > 0">-</span>
      {{ price | currency : 'R$ ' : 2 }}
    </span>
  </div>
  <div class="gift-quantity col-md-3" ng-show="gift.has_many_packages()">
    <strong>Qtn:</strong>
    {{ gift.quantity }}
  </div>
</div>
