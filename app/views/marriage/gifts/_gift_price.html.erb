<div class="row">
  <div class="gift-price col-md-7" ng-show="gift.price_range.length > 0">
    <strong>Preço<span ng-show="gift.quantity > 1"> Total</span>: </strong>
    <span ng-repeat="price in gift.price_range track by $index">
      <span ng-show="$index > 0">até</span>
      {{ price | currency : 'R$ ' : 2 }}
    </span>
  </div>
  <div class="gift-quantity col-md-5" ng-show="gift.quantity > 1">
    <strong>Quantidade:</strong>
    {{ gift.quantity }}
  </div>
</div>
