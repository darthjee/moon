<div class="panel panel-default show-block debug-container">
  <%= render partial: 'marriage/pictures/index/debug' %>
  <div class="panel-heading picture-name">
    {{ picture.name }}&nbsp;
  </div>
  <div class="panel-body">
    <div class="row">
      <div class="col-sm-12">
        <div class="show-image-block">
          <a href="{{ picture.url }}" target="_BLANK">
            <img src="{{ picture.snap_url }}" class="picture-image" />
          </a>
        </div>
      </div>
    </div>
  </div>
</div>
